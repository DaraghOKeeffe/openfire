<?xml version="1.0" encoding="UTF-8"?>
<plugin>
    <!-- Main plugin class -->
    <class>ie.ucc.plugin.SoapBoxPlugin</class>

    <!-- Plugin meta-data -->
    <name>Soap Box</name>
    <description>This is an example plugin. It adds soapbox.com to whitelist</description>
    <author>Wayne</author>

    <version>1.0</version>
    <date>08/06/2010</date>
    <url>soapbox.com</url>
    <minServerVersion>3.0.0</minServerVersion>
    <licenseType>gpl</licenseType>

    <adminconsole>		
        <tab id="tab-server">
            <sidebar id="sidebar-server-settings">
                <item id="soapBox" name="Soap Box" 
                     description="Empty" />
            </sidebar>
        </tab>
    </adminconsole>
    
</plugin>